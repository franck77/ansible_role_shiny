<!-- Sign&go WEB agent Configuration file -->

<!-- Info: To comment out the parameters (to ignore them), you can use XML comments.
     Note: In this file, some elements are de-activated by prepending two underscores to the TAG name. (ex: replace "__FilteredUrls"  by  "FilteredUrls" to activate)
 -->
<Config>
  <Agent>
	<!-- Change between env -->
	<Name>SG_APACHEAPP_PRO</Name>
	<!-- Change between env -->
    <Pass>Ciphered:AAEAELj1PebAiZgUnX4y9n0KRJkAAgAgqli8n0FOe0EoUfs52ed2zoORl793ghTw4/K8RrufuCE=</Pass>

    <AgentLogMask>0x07000386</AgentLogMask>
    <LogLevel>5</LogLevel>

    <AgentLogFile1>sngLogAg1.log</AgentLogFile1>
